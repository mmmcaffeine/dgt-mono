{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Repositories": {
    "Contact": {
      "Path": ".\\Data\\contacts.json",
      "Delay": 500
    },
    "Branch": {
      "Path": ".\\Data\\branches.json",
      "Delay": 1500
    }
  },
  "CircuitBreakers": {
    "ContactRepository": {
      "Attempts": 1,
      "Duration": "00:05:00"
    }
  },
  "ConnectionStrings": {
    "Redis": "This is a secret"
  },
  "AllowedHosts": "*"
}
